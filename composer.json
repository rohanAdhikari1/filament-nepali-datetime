{
    "name": "rohanadhikari/filament-nepali-datetime",
    "description": "A wrapper for Filament Component like FilamentDateTimePicker,TextColumn,TextEntry to support Nepali Date.",
    "keywords": [
        "rohanadhikari",
        "laravel",
        "filament-nepali-datetime"
    ],
    "homepage": "https://github.com/rohanadhikari1/filament-nepali-datetime",
    "support": {
        "issues": "https://github.com/rohanadhikari1/filament-nepali-datetime/issues",
        "source": "https://github.com/rohanadhikari1/filament-nepali-datetime"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rohan Adhikari",
            "email": "official@rohanadhikari.com.np",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/forms": "^4.0",
        "filament/infolists": "^4.0",
        "filament/tables": "^4.0",
        "rohanadhikari/nepali-date": "^0.0.3"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "RohanAdhikari\\FilamentNepaliDatetime\\": "src/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "refactor": "vendor/bin/rector"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RohanAdhikari\\FilamentNepaliDatetime\\FilamentNepaliDatetimeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
